{
    "name": "@athenz/ui",
    "version": "0.4.0",
    "description": "React-based UI for Athenz",
    "repository": {
        "type": "git",
        "url": "git@github.com:AthenZ/athenz.git"
    },
    "contributors": [
        {
            "name": "Athenz Team",
            "email": "athenz@verizonmedia.com"
        }
    ],
    "license": "Apache-2.0",
    "keywords": [
        "Athenz",
        "rbac",
        "role-based-access-control",
        "authorization",
        "athenz ui"
    ],
    "main": "app.js",
    "bugs": {
        "url": "https://github.com/AthenZ/athenz/issues"
    },
    "scripts": {
        "dev": "DEBUG=AthenzUI:* next dev",
        "test": "APP_ENV=unittest jest --coverage --no-cache",
        "regen-snap": "jest --updateSnapshot",
        "build": "NODE_ENV=production next build",
        "start": "NODE_ENV=production node app.js",
        "fix-lint": "prettier --write *.js src/** __mocks__/*/*.js",
        "single-test": "jest -i src/__tests__/pages/index.test.js -t HomePage",
        "func:local:wd": "webdriver-manager update && webdriver-manager start --seleniumPort 4445",
        "func:local:ui": "DEBUG=AthenzUI:functional:* APP_ENV=functional-local PLATFORM=$(uname -s) wdio run src/__tests__/spec/wdio.conf.js --baseUrl=$BASEURL",
        "ci-setup": "./scripts/ci/ci-setup.sh",
        "ci-build": "./scripts/ci/ci-remove-optional.sh && npm run build",
        "ci-cleanup": "./scripts/ci/ci-cleanup.sh",
        "ci-deploy": "./scripts/ci/ci-deploy.sh",
        "ci-func-test": "./scripts/ci/ci-run-functional.sh",
        "ci-lint": "prettier --list-different *.js src/** __mocks__/*/*.js",
        "ci-unit-test": "jest -i",
        "functional": "wdio run src/__tests__/spec/wdio.conf.js"
    },
    "dependencies": {
        "@athenz/auth-core": "1.0.0",
        "@emotion/cache": "11.7.1",
        "@emotion/core": "11.0.0",
        "@emotion/css": "11.7.1",
        "@emotion/react": "11.7.1",
        "@emotion/server": "11.4.0",
        "@emotion/styled": "11.6.0",
        "async": "3.2.2",
        "body-parser": "1.19.1",
        "cookie-parser": "1.4.6",
        "cookie-session": "2.0.0",
        "csurf": "1.11.0",
        "debug": "4.3.3",
        "denali-css": "2.5.1",
        "downshift": "6.1.7",
        "emotion": "11.0.0",
        "express": "4.17.2",
        "fetchr": "0.5.45",
        "flatpickr": "4.6.9",
        "helmet": "4.6.0",
        "js-cookie": "2.2.1",
        "lodash": "4.17.21",
        "lodash.clone": "4.5.0",
        "moment": "2.29.1",
        "moment-timezone": "0.5.33",
        "multer": "1.4.4",
        "next": "12.1.0",
        "passport": "0.5.2",
        "passport-strategy": "1.0.0",
        "polished": "4.1.3",
        "prop-types": "15.8.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-modal": "3.14.4",
        "react-popper": "2.2.5",
        "react-popper-tooltip": "3.1.1",
        "request": "2.88.2",
        "setimmediate": "1.0.5",
        "terser": "5.10.0",
        "tslib": "2.3.1",
        "typescript": "4.5.4",
        "uuid": "8.3.2",
        "xml-crypto": "2.1.3",
        "@denali-design/icons": "1.1.0",
        "@denali-design/react": "1.0.0-alpha.1",
        "cytoscape": "3.20.0",
        "cytoscape-dagre": "2.3.2",
        "react-flow-renderer": "9.7.2"
    },
    "devDependencies": {
        "@babel/core": "7.16.7",
        "@emotion/babel-plugin": "11.7.2",
        "@emotion/jest": "11.7.1",
        "@sinonjs/referee": "7.0.0",
        "@testing-library/jest-dom": "5.16.1",
        "@testing-library/react": "11.2.7",
        "@wdio/cli": "7.16.12",
        "@wdio/junit-reporter": "7.16.12",
        "@wdio/local-runner": "7.16.12",
        "@wdio/mocha-framework": "7.16.11",
        "@wdio/sauce-service": "7.16.12",
        "@wdio/selenium-standalone-service": "7.16.11",
        "@wdio/spec-reporter": "7.16.11",
        "babel-jest": "27.4.5",
        "chromedriver": "96.0.0",
        "dotenv": "10.0.0",
        "enzyme": "3.11.0",
        "expect": "26.6.2",
        "expect-webdriverio": "1.4.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "26.6.3",
        "jest-emotion": "11.0.0",
        "jest-junit": "13.0.0",
        "jsdom": "16.5.3",
        "mutationobserver-shim": "0.3.7",
        "node-mocks-http": "1.11.0",
        "prettier": "2.5.1",
        "react-test-renderer": "17.0.2",
        "sinon": "9.2.3",
        "supertest": "6.1.6",
        "wdio-chromedriver-service": "7.2.4",
        "webdriver-manager": "12.1.8",
        "webdriverio": "7.16.12",
        "follow-redirects": "1.14.8",
        "nanoid": "3.1.31"
    }
}
