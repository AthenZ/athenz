{
    "name": "@athenz/ui",
    "version": "0.4.0",
    "description": "React-based UI for Athenz",
    "repository": {
        "type": "git",
        "url": "git@github.com:AthenZ/athenz.git"
    },
    "contributors": [
        {
            "name": "Athenz Team",
            "email": "athenz@verizonmedia.com"
        }
    ],
    "license": "Apache-2.0",
    "keywords": [
        "Athenz",
        "rbac",
        "role-based-access-control",
        "authorization",
        "athenz ui"
    ],
    "main": "app.js",
    "bugs": {
        "url": "https://github.com/AthenZ/athenz/issues"
    },
    "scripts": {
        "dev": "DEBUG=AthenzUI:* next dev",
        "test": "APP_ENV=unittest jest --coverage --no-cache",
        "regen-snap": "jest --updateSnapshot",
        "build": "NODE_ENV=production next build",
        "start": "NODE_ENV=production node app.js",
        "fix-lint": "prettier --write *.js src/*.js src/config/*.js src/pages/*.js src/components/*/*.js src/server/*.js src/server/*/*.js src/__tests__/*/*.js __mocks__/*/*.js",
        "single-test": "jest -i src/__tests__/pages/home.test.js -t HomePage",
        "func:local:wd": "webdriver-manager update && webdriver-manager start --seleniumPort 4445",
        "func:local:ui": "DEBUG=AthenzUI:functional:* APP_ENV=functional-local PLATFORM=$(uname -s) wdio run src/__tests__/spec/wdio.conf.js --baseUrl=$BASEURL",
        "ci-setup": "./scripts/ci/ci-setup.sh",
        "ci-build": "./scripts/ci/ci-remove-optional.sh && npm run build",
        "ci-cleanup": "./scripts/ci/ci-cleanup.sh",
        "ci-deploy": "./scripts/ci/ci-deploy.sh",
        "ci-func-test": "./scripts/ci/ci-run-functional.sh",
        "ci-lint": "prettier --list-different *.js src/*.js src/config/*.js src/pages/*.js src/components/*/*.js src/server/*.js src/server/*/*.js src/__tests__/*/*.js __mocks__/*/*.js",
        "ci-unit-test": "jest -i",
        "functional": "wdio run src/__tests__/spec/wdio.conf.js"
    },
    "dependencies": {
        "@athenz/auth-core": "^1.0.0",
        "@emotion/cache": "^11.1.3",
        "@emotion/core": "^11.0.0",
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.1.4",
        "@emotion/server": "^11.0.0",
        "@emotion/styled": "^11.3.0",
        "async": "^3.2.0",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "cookie-session": "^2.0.0-rc.1",
        "csurf": "^1.11.0",
        "debug": "^4.3.1",
        "denali-css": "^2.3.2",
        "downshift": "^6.1.3",
        "emotion": "^11.0.0",
        "express": "^4.17.1",
        "fetchr": "^0.5.41",
        "flatpickr": "4.6.8",
        "helmet": "^4.5.0",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.20",
        "lodash.clone": "^4.5.0",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.32",
        "multer": "^1.4.2",
        "next": "^10.0.5",
        "next-compose-plugins": "^2.2.1",
        "next-routes": "^1.4.2",
        "passport": "^0.4.1",
        "passport-strategy": "^1.0.0",
        "polished": "^4.1.2",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-modal": "^3.13.1",
        "react-popper": "^2.2.4",
        "react-popper-tooltip": "^3.1.1",
        "request": "^2.88.2",
        "setimmediate": "^1.0.5",
        "terser": "^5.7.0",
        "uuid": "^8.3.2",
        "xml-crypto": "^2.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.13.16",
        "@emotion/babel-plugin": "^11.3.0",
        "@emotion/jest": "^11.3.0",
        "@sinonjs/referee": "^7.0.0",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@wdio/cli": "^6.11.3",
        "@wdio/junit-reporter": "^6.11.0",
        "@wdio/local-runner": "^6.11.3",
        "@wdio/mocha-framework": "^6.11.0",
        "@wdio/sauce-service": "^6.11.0",
        "@wdio/selenium-standalone-service": "^6.11.0",
        "@wdio/spec-reporter": "^6.11.0",
        "babel-jest": "^26.6.3",
        "chromedriver": "^87.0.5",
        "dotenv": "^8.2.0",
        "enzyme": "^3.11.0",
        "expect": "^26.6.2",
        "expect-webdriverio": "^1.4.1",
        "identity-obj-proxy": "^3.0.0",
        "jenkins-mocha": "^8.0.0",
        "jest": "^26.6.3",
        "jest-emotion": "^11.0.0",
        "jest-junit": "^12.0.0",
        "jsdom": "^16.5.3",
        "mutationobserver-shim": "^0.3.7",
        "node-mocks-http": "^1.9.0",
        "prettier": "^2.3.0",
        "sinon": "^9.2.3",
        "supertest": "^6.0.1",
        "wdio-chromedriver-service": "^6.0.4",
        "webdriver-manager": "^12.1.8",
        "webdriverio": "^6.11.3"
    }
}
